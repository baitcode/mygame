{
    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "React": true
    },

    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
    },

    "extends": [
        "standard",
        "standard-react"
    ],

    "plugins": [
        "react"
    ]
}
